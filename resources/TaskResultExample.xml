<component-task-result xmlns="http://java.sun.com/xml/ns/jbi/management-message">
    <component-name>B2BBC</component-name>
    <component-task-result-details>
        <task-result-details>
            <task-id>MyTaskID</task-id>
            <task-result>SUCCESS</task-result>
            <message-type>INFO</message-type>
            <task-status-message>
                <msg-loc-info>
                    <loc-token>MyLocToken01</loc-token>
                    <loc-message>MyLocMessage01</loc-message>
                    <loc-parameter>p1</loc-parameter>
                    <loc-parameter>p2</loc-parameter>
                </msg-loc-info>
            </task-status-message>
            <exception-info>
                <nesting-level>1</nesting-level>
                <msg-loc-info>
                    <loc-token>MyLocToken02</loc-token>
                    <loc-message>MyLocMessage02</loc-message>
                    <loc-parameter>p11</loc-parameter>
                    <loc-parameter>p21</loc-parameter>
                </msg-loc-info>
                <stack-trace>This is my stack trace</stack-trace>
            </exception-info>
        </task-result-details>
    </component-task-result-details>
</component-task-result>
